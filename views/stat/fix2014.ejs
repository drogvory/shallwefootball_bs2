			  <li><a href="#DAY12_2014" data-toggle="tab" data-schedule="10/25">Semi-Finals</a></li>
			  <li><a href="#DAY13_2014" data-toggle="tab" data-schedule="11/1">Final</a></li>
			</ul>




			<div class="tab-content">

					<% var day = 1; %>
					<% var match = 0;%>
					<% index.matches.forEach(function (item) { %>
						<% if(!(match%4)) { %>
							<div class="tab-pane fade" id="DAY<%= day %>_2014" >
								<table class="table">
							      <thead>
							        <tr>
							          <th>일정 및 장소</th>
							          <th>시간</th>
							          <th>대진 및 결과</th>
							          <th>기록</th>
							        </tr>
							      </thead>
						<% } %>
									<tr>
										<% if(!(match%2)) { %>
											<td rowspan=2>
												<%= item.month %>.<%= item.day %> <%= item.stadium %>
											</td>
										<% } %>
											<td>
												<%= item.hour %>.00~
											</td>

										<% if(!item.homeScore && !item.awayScore) { %>
											<td>
												<%= item.homeClubName %> () : () <%= item.awayClubName %>
											</td>
										<% }else { %>
											<td>
												<%= item.homeClubName %> (<%= item.homeScore %>) : (<%= item.awayScore %>) <%= item.awayClubName %>
											</td>
										<% } %>

										<% if (item.link) { %>
											<td><a class="detailStat" target="_blank" href="<%= item.link %>">자세히</a></td>
										<% }else { %>
											<td><%= item.note ? item.note : item.matchName %></td>
										<% } %>
									</tr>

						<% if(match%4 == 3 || match == 48) { %>
							    </table>
							</div>
							<% day++; %>
						<% } %>
					<% match++; %>
					<% }) %>

			</div>

	</div>

	<div class="modal-footer">
		<button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
	</div>


	<script>
		var now = new Date();
		// console.log('fixDay     : ', fixDay);
		$('#nav2014 li').each(function (i) {
			var schedule = $('#nav2014 li').eq(i).children().attr("data-schedule");
			var month = schedule.split('/')[0];
			var day = schedule.split('/')[1];
			var fixDay = new Date(2014, (month-1), day);

			if(fixDay > now) {

				$(this).addClass("active");
				var matchDay = ($(this)).children().attr("href");

				$(matchDay).addClass("in active");

				return false;	//jquery return을 하려면 false 해줘야함
			}

		})
	</script>

</div>


